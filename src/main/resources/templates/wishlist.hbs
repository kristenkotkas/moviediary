                </thead>
                <tbody id="wishlist-table" class="grey-text">
                </tbody>
            </table>
        </div>
    </div>
</div>

<script type="text/javascript" src="/static/js/fallback.min.js"></script>
<script async type="text/javascript" src="/static/js/loaders/ui.js"></script>
